<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yc.meituan.mapper.SellerInfoMapper" >

	<select id="login" parameterType="SellerInfo" resultType="SellerInfo">
		select * from MEITUAN_SELLER_INFO where saccounts = #{saccounts} and spwd = #{spwd}
	</select>
	
	<select id="selectSBackShowGoods" parameterType="GoodsInfo" resultType="GoodsInfo">
		select gid,gtitle1,gdate,gprice1,gprice2,privilege,gsoldcount,temp1 from meituan_goods_info where sid=#{sid}
	</select>
	
</mapper>